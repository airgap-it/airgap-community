{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "repository": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string"
        },
        "commit": {
          "type": "string"
        }
      },
      "required": [
        "url",
        "commit"
      ]
    },
    "build": {
      "type": "string"
    },
    "manifest": {
      "type": "string"
    }
  },
  "required": [
    "repository",
    "build",
    "manifest"
  ]
}